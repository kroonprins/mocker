{
  "name": "mocker",
  "version": "1.0.0",
  "repository": "https://github.com/kroonprins/mocker",
  "description": "Mock server with learning mode",
  "main": "index.mjs",
  "scripts": {
    "//": "Use extremly basic runner for tests until one of the frameworks supports mjs...",
    "test": "node --experimental-modules test/runner.mjs; rc=$?; rm -fr ./test/tmp; exit $rc",
    "postinstall": "rm -fr .git/hooks && ln -fs ../.githooks/ .git/hooks; mkdir -p ./lib/mjs_workaround && ln -f ./node_modules/serializr/lib/es/serializr.js lib/mjs_workaround/serializr-es6-module-loader.mjs",
    "version-check": "npm-check-updates",
    "lint-check": "eslint index.mjs lib/*.mjs test/**/*.mjs",
    "lint-fix": "eslint index.mjs lib/*.mjs test/**/*.mjs --fix",
    "doc": "jsdoc -c jsdoc-conf.json"
  },
  "author": "kroonprins",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.17.1",
    "chai": "^4.1.2",
    "chai-exclude": "^1.0.3",
    "chai-string": "^1.4.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdoc": "^3.5.5",
    "npm-check-updates": "^2.13.0",
    "portastic": "^1.0.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-async-errors": "^2.1.1",
    "glob": "^7.1.2",
    "http-proxy-middleware": "^0.17.4",
    "js-yaml": "^3.10.0",
    "mem": "^3.0.0",
    "mkdirp": "^0.5.1",
    "nedb": "^1.8.0",
    "nunjucks": "^3.0.1",
    "pino": "^4.10.2",
    "rimraf": "^2.6.2",
    "serializr": "^1.1.14",
    "tough-cookie": "^2.3.3",
    "uuid": "^3.1.0"
  }
}
