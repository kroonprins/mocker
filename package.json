{
  "name": "@kroonprins/mocker",
  "version": "0.0.14",
  "repository": "https://github.com/kroonprins/mocker",
  "description": "Mock server with learning mode",
  "main": "mocker-ui.mjs",
  "scripts": {
    "start": "./mocker-ui.mjs",
    "//": "Use extremly basic runner for tests until one of the frameworks supports mjs...",
    "test": "node --experimental-modules test/runner.mjs",
    "posttest": "node build-scripts/post-test.js",
    "postinstall": "node build-scripts/post-install.js",
    "version-check": "npm-check-updates",
    "lint-check": "eslint *.mjs lib/*.mjs test/**/*.mjs",
    "lint-fix": "eslint *.mjs lib/*.mjs test/**/*.mjs --fix",
    "doc": "jsdoc -c jsdoc-conf.json",
    "ui": "./mocker-ui.mjs",
    "mock-server": "./mocker-mock-server.mjs",
    "learning-mode": "./mocker-learning-mode.mjs",
    "mock-server-swagger-ui": "./mocker-mock-server-swagger-ui.mjs",
    "docker-build": "cross-var docker build -t kroonprins/mocker:latest -t kroonprins/mocker:$npm_package_version .",
    "docker-test": "docker run kroonprins/mocker npm test",
    "docker-push": "docker push kroonprins/mocker"
  },
  "author": "kroonprins",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "chai-exclude": "^1.0.9",
    "chai-string": "^1.5.0",
    "colors": "^1.3.2",
    "columnify": "^1.5.4",
    "cross-var": "^1.1.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.5.5",
    "npm-check-updates": "^3.0.0-alpha2",
    "portastic": "^1.0.1"
  },
  "dependencies": {
    "ajv": "^6.5.3",
    "ajv-async": "^1.0.1",
    "body-parser": "^1.18.3",
    "body-parser-xml": "^1.1.0",
    "chokidar": "^2.0.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "express-delay": "^0.2.0",
    "glob": "^7.1.3",
    "http-proxy-middleware": "^0.19.0",
    "js-yaml": "^3.12.0",
    "mem": "^4.0.0",
    "mkdirp": "^0.5.1",
    "nedb": "^1.8.0",
    "normalize-path": "^3.0.0",
    "nunjucks": "^3.1.3",
    "pino": "^5.4.1",
    "rimraf": "^2.6.2",
    "serializr": "^1.3.0",
    "swagger-ui-express": "^4.0.1",
    "tough-cookie": "^2.4.3",
    "uuid": "^3.3.2"
  },
  "bin": {
    "mocker-ui": "./mocker-ui.mjs",
    "mocker-mock-server": "./mocker-mock-server.mjs",
    "mocker-learning-mode": "./mocker-learning-mode.mjs"
  }
}
