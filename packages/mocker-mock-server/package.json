{
  "name": "@kroonprins/mocker-mock-server",
  "version": "0.1.12",
  "description": "Mock server",
  "main": "exports.mjs",
  "scripts": {
    "prepare": "npm run create-cjs",
    "start": "./start.mjs",
    "test": "mocker-test-runner",
    "create-cjs": "./babel.sh",
    "test-cjs": "mocker-test-runner-cjs",
    "version-check": "updates",
    "docker-build": "cross-var docker build -t kroonprins/mocker-mock-server:latest -t kroonprins/mocker-mock-server:$npm_package_version .",
    "docker-test": "docker run --rm kroonprins/mocker-mock-server npm test",
    "docker-push": "docker push kroonprins/mocker-mock-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kroonprins/mocker.git"
  },
  "author": "kroonprins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kroonprins/mocker/issues"
  },
  "homepage": "https://github.com/kroonprins/mocker#readme",
  "files": [
    "exports.mjs",
    "start.mjs",
    "src/**",
    "cjs/exports.cjs",
    "cjs/src/**"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@kroonprins/mocker-shared-lib": "^0.1.12",
    "body-parser": "^1.19.0",
    "body-parser-xml": "^1.1.0",
    "chokidar": "^3.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "events": "^3.0.0",
    "express": "^4.17.1",
    "express-timestamp": "^0.1.4",
    "js-yaml": "^3.13.1",
    "mem": "^6.0.1",
    "moment-timezone": "^0.5.27",
    "nunjucks": "^3.2.0",
    "path": "^0.12.7",
    "serializr": "^1.5.4",
    "swagger-ui-express": "^4.1.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "axios": "^0.19.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-import-meta": "^1.0.0",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "chai-string": "^1.5.0",
    "cross-var": "^1.1.0",
    "portastic": "^1.0.1",
    "updates": "^9.3.3"
  },
  "bin": {
    "mocker-mock-server": "./start.mjs"
  }
